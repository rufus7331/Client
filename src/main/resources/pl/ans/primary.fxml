      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <CheckBox fx:id="answer1" mnemonicParsing="false" text="CheckBox">
               <font>
                  <Font size="14.0" />
               </font></CheckBox>
            <CheckBox fx:id="answer2" mnemonicParsing="false" text="CheckBox" />
            <CheckBox fx:id="answer3" mnemonicParsing="false" text="CheckBox" />
            <CheckBox fx:id="answer4" mnemonicParsing="false" text="CheckBox" />
         </children>
      </HBox>
      <Button fx:id="primaryButton" onAction="#answer" text="NastÄ™pne pytanie" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
